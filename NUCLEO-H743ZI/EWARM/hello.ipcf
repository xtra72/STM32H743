<?xml version="1.0" encoding="UTF-8"?>
<!-- IAR Project Connection File -->

<iarProjectConnection version="1.2" name="Hello Example" oneShot="true"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="IarProjectConnection_1.2.xsd">

  <device>
    <name>CC1310F128</name>
  </device>

  <extraOptions>
    <args>
      <arg>-f</arg>
      <arg>$PROJ_DIR$/configPkg/compiler.opt.defs</arg>
      <arg>-I$PROJ_DIR$</arg>
      <arg>-DDeviceFamily_CC13X0</arg>
      <arg>-I$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/source</arg>
      <arg>-I$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/source/ti/posix/iar</arg>
      <arg>--debug</arg>
      <arg>--silent</arg>
      <arg>-e</arg>
      <arg>--aeabi</arg>
      <arg>--thumb</arg>
      <arg>--diag_suppress=Pa050</arg>
    </args>
  </extraOptions>

  <asmIncludePath>
    <path>$PROJ_DIR$</path>
  </asmIncludePath>

  <asmDefines>
  </asmDefines>

  <linkerFile>
    <override>true</override>
    <path>$PROJ_DIR$/$PROJ_FNAME$_CC1310_LAUNCHXL_TIRTOS.icf</path>
  </linkerFile>

  <linkerExtraOptions>
    <args>
      <arg>-f</arg>
      <arg>$PROJ_DIR$/configPkg/linker.cmd</arg>
      <arg>$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/source/ti/display/lib/display.arm3</arg>
      <arg>$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/source/ti/grlib/lib/iar/m3/grlib.a</arg>
      <arg>$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/source/third_party/spiffs/lib/iar/m3/spiffs_cc26xx.a</arg>
      <arg>$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/source/ti/drivers/rf/lib/rf_multiMode_cc13x0.arm3</arg>
      <arg>$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/source/ti/drivers/lib/drivers_cc13x0.arm3</arg>
      <arg>$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/kernel/tirtos/packages/ti/dpl/lib/dpl_cc13x0.arm3</arg>
      <arg>$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/source/ti/devices/cc13x0/driverlib/bin/iar/driverlib.lib</arg>
      <arg>--silent</arg>
      <arg>--cpu=Cortex-M3</arg>
    </args>
  </linkerExtraOptions>

    <customBuildTools>
    <customBuildTool name="configuro">   <!-- Custom build tool that will be assigned to a file group -->
      <fileExtensions>.cfg</fileExtensions>
      <command>"$XDCROOT$/bin/stderr"</command>
      <output>
        <path>$PROJ_DIR$/configPkg/package.mak</path>
      </output>
    </customBuildTool>
    </customBuildTools>

  <heaps>
    <heap> <!-- If omitted, id="0" -->
      <size>0</size>
    </heap>
    <heap id="1">
      <size>0</size>
    </heap>
  </heaps>
  <stacks>
    <stack> <!-- If omitted, id="0" -->
      <size>512</size>
    </stack>
  </stacks>

  <preBuild>
    <command>"$XDCROOT$/xs" --xdcpath="$XDCPATH$" iar.tools.configuro -c "$TOOLKIT_DIR$" --cc "$COMPILER_PATH$" --device "$DEVICE$" --compileOptions $COMPILER_ARGS_ROOT_QUOTED$ --linkOptions $LINKER_ARGS_QUOTED$ --profile release --projFile "$PROJ_PATH$"</command>
  </preBuild>

  <files>
    <group name="Source files">
      <path copyTo="$PROJ_DIR$/hello.c">$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/sysbios/hello/hello.c</path>
      <path copyTo="$PROJ_DIR$/CC1310_LAUNCHXL.c">$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/sysbios/hello/CC1310_LAUNCHXL.c</path>
      <path copyTo="$PROJ_DIR$/CC1310_LAUNCHXL_fxns.c">$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/sysbios/hello/CC1310_LAUNCHXL_fxns.c</path>
      <path copyTo="$PROJ_DIR$/ccfg.c">$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/sysbios/hello/ccfg.c</path>
      <path copyTo="$PROJ_DIR$/$PROJ_FNAME$_CC1310_LAUNCHXL_TIRTOS.icf">$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/sysbios/hello/tirtos/iar/CC1310_LAUNCHXL_TIRTOS.icf</path>
    </group>

    <group name="Header files">
      <path copyTo="$PROJ_DIR$/Board.h">$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/sysbios/hello/Board.h</path>
      <path copyTo="$PROJ_DIR$/CC1310_LAUNCHXL.h">$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/sysbios/hello/CC1310_LAUNCHXL.h</path>
    </group>

    <group name="Configuration files" customBuildTool="configuro">
       <path copyTo="$PROJ_DIR$/$PROJ_FNAME$.cfg">$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/sysbios/hello/tirtos/hello.cfg</path>
    </group>

    <group name="Documentation" build="false">
      <path copyTo="$PROJ_DIR$/README.md">$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/sysbios/hello/README.md</path>
      <path copyTo="$PROJ_DIR$/README.html">$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/examples/rtos/CC1310_LAUNCHXL/sysbios/hello/README.html</path>
      <path reference="$PROJ_DIR$/Board.html">$SIMPLELINK_CC13X0_SDK_INSTALL_DIR$/source/ti/boards/CC1310_LAUNCHXL/Board.html</path>
    </group>
  </files>
</iarProjectConnection>
